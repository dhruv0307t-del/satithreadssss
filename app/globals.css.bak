@tailwind base;
@tailwind components;
@tailwind utilities;

/* ================= BASE RESET ================= */

:root {
  --luxury-brown: #3b1f23;
  --luxury-amber: #FFF7EC;
  --text-light: black;
  --text-dark: #2c1a1a;
}

* {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}

html {
  scroll-behavior: smooth;
}

body {
  font-family: "Inter", system-ui, -apple-system, BlinkMacSystemFont, sans-serif;
  background: #FFF7EC;
  color: var(--text-dark);
  -webkit-font-smoothing: antialiased;
}

/* ================= SCROLLBAR ================= */

::-webkit-scrollbar {
  width: 8px;
}

::-webkit-scrollbar-track {
  background: #FFF7EC;
}

::-webkit-scrollbar-thumb {
  background: #3b1f23;
  border-radius: 6px;
}

/* ================= MORPH LOGO ================= */

.global-logo {
  position: fixed;
  top: calc(50% + 36px);
  left: 50%;
  transform: translate(-50%, -50%) scale(1.6);
  z-index: 100;
  pointer-events: none;
  transition:
    transform 0.95s cubic-bezier(0.19, 1, 0.22, 1),
    top 0.95s cubic-bezier(0.19, 1, 0.22, 1),
    left 0.95s cubic-bezier(0.19, 1, 0.22, 1);

  will-change: transform, top, left;
  backface-visibility: hidden;
}

.global-logo img {
  image-rendering: auto;
  filter: drop-shadow(0 6px 14px rgba(0, 0, 0, 0.18));
}

.global-logo.to-nav {
  position: fixed;
  top: 36px;
  left: 48px;
  transform: translate(0, -50%) scale(0.42);
}


/* ================= NAVBAR ================= */

.navbar {
  position: fixed;
  top: 36px;
  left: 50%;
  transform: translateX(-50%);

  width: fit-content;
  min-width: 880px;
  max-width: 1180px;

  display: flex;
  align-items: center;
  justify-content: space-between;

  padding: 14px 28px;
  border-radius: 999px;

  background: rgba(255, 255, 255, 0.78);
  backdrop-filter: blur(20px);
  -webkit-backdrop-filter: blur(20px);

  box-shadow: 0 12px 40px rgba(0, 0, 0, 0.14);
  z-index: 1000;

  transition: opacity 0.4s ease, transform 0.4s ease;
}

.nav-left-spacer {
  width: 180px;
  height: 72px;
}

.nav-links {
  list-style: none;
  display: flex;
  gap: 36px;
  justify-content: center;
  color: #3b1f23;
  font-size: 15px;
  font-weight: 500;
}

.nav-links li {
  font-size: 14px;
  letter-spacing: 0.06em;
  cursor: pointer;
  transition: opacity 0.2s ease;
}

.nav-links li:hover {
  opacity: 0.7;
}

.nav-left {
  flex: 1;
}

.nav-links {
  flex: 2;
  justify-content: center;
}

.nav-right {
  flex: 1;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  gap: 16px;
}

.nav-right span {
  margin-left: 20px;
  color: #3b1f23;
  cursor: pointer;
}

.nav-icon {
  color: #3b1f23;
  cursor: pointer;
  transition: opacity 0.2s ease, transform 0.2s ease;
}

.nav-icon:hover {
  opacity: 0.7;
  transform: translateY(-1px);
}

.nav-hidden {
  opacity: 0;
  pointer-events: none;
  transform: translate(-50%, -10px);
}

.nav-visible {
  opacity: 1;
  pointer-events: auto;
  transform: translate(-50%, 0);
  transition: opacity 0.4s ease, transform 0.4s ease;
}

.nav-enter {
  animation: navFade 0.6s ease forwards;
  animation-delay: 0.15s;
}

.navbar img {
  transform: translateY(2px);
}


/* ================= HERO - FIXED Z-INDEX ================= */

.hero-banner {
  height: 100vh;
  width: 100%;
  margin-top: 0;
  position: relative;
  overflow: hidden;
  background: transparent;
  opacity: 0;
  transform: translateY(20px);
  animation: fadeInUp 0.8s ease forwards;
  animation-delay: 0.3s;
  padding-top: 0;
  z-index: 1 !important;
  /* FIXED: Ensure hero is below navbar */
}

.hero-image {
  object-fit: cover !important;
  object-position: center top !important;
  filter: contrast(1.05) saturate(1.05);
  position: absolute !important;
  top: 0 !important;
  left: 0 !important;
  width: 100% !important;
  height: 100% !important;
}

.hero-luxury-overlay {
  position: absolute;
  inset: 0;
  background: linear-gradient(to bottom,
      rgba(0, 0, 0, 0.15),
      rgba(0, 0, 0, 0.35));
  display: flex;
  align-items: center;
  padding-left: 8vw;
  z-index: 2;
}

.hero-banner::after {
  content: "";
  position: absolute;
  inset: 0;
  box-shadow: inset 0 -120px 140px rgba(0, 0, 0, 0.35);
  pointer-events: none;
  z-index: 3;
}

.hero-copy {
  max-width: 520px;
}

.hero-eyebrow {
  font-size: 13px;
  letter-spacing: 0.32em;
  color: #3b1f23;
  margin-bottom: 18px;
}

.hero-copy h1 {
  font-family: "Playfair Display", serif;
  font-size: 54px;
  line-height: 1.15;
  color: #3b1f23;
  margin-bottom: 36px;
}

.hero-cta {
  padding: 14px 36px;
  border-radius: 999px;
  border: none;
  background: #3b1f23;
  color: #fff;
  cursor: pointer;
}

@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(20px);
  }

  to {
    opacity: 1;
    transform: translateY(0);
  }
}

/* ================= CATEGORIES - FIXED Z-INDEX ================= */

.categories-section {
  padding: 100px 0;
  background: #FFF7EC !important;
  margin-top: 0;
  opacity: 0;
  transform: translateY(20px);
  animation: fadeInUp 0.8s ease forwards;
  animation-delay: 0.6s;
}

.categories-title {
  text-align: center;
  font-size: 72px;
  color: #3b1f23 !important;
  margin-bottom: 60px;
  font-family: "Playfair Display", serif;
  font-weight: 900;
  letter-spacing: 0.05em;
  text-transform: uppercase;
}

/* Carousel Container */
.categories-carousel-container {
  max-width: 1600px;
  margin: 0 auto;
  padding: 0 60px;
  position: relative;
}

/* Aesthetic Box with Bold Borders */
.categories-carousel-box {
  border: 4px solid #3b1f23;
  border-radius: 24px;
  padding: 40px 30px;
  background: linear-gradient(135deg, #FFFBF5 0%, #FFF7EC 100%);
  box-shadow:
    0 10px 40px rgba(59, 31, 35, 0.12),
    inset 0 1px 0 rgba(255, 255, 255, 0.8);
  position: relative;
  overflow: visible;
}

/* Decorative corner elements */
.categories-carousel-box::before {
  content: '';
  position: absolute;
  top: 20px;
  left: 20px;
  width: 60px;
  height: 60px;
  border-top: 3px solid #f4c542;
  border-left: 3px solid #f4c542;
  opacity: 0.7;
}

.categories-carousel-box::after {
  content: '';
  position: absolute;
  bottom: 20px;
  right: 20px;
  width: 60px;
  height: 60px;
  border-bottom: 3px solid #f4c542;
  border-right: 3px solid #f4c542;
  opacity: 0.7;
}

/* Smooth Scroll Wrapper */
.categories-scroll-wrapper {
  overflow-x: auto;
  overflow-y: hidden;
  scroll-behavior: smooth;
  cursor: grab;
  user-select: none;
  -webkit-overflow-scrolling: touch;

  /* Hide scrollbar but keep functionality */
  scrollbar-width: none;
  /* Firefox */
  -ms-overflow-style: none;
  /* IE and Edge */
}

.categories-scroll-wrapper::-webkit-scrollbar {
  display: none;
  /* Chrome, Safari, Opera */
}

.categories-scroll-wrapper:active {
  cursor: grabbing;
}

/* Grid for smooth scrolling */
.categories-grid-scroll {
  display: flex;
  gap: 20px;
  padding: 10px 0;
  width: max-content;
}

.category-card-new {
  position: relative;
  cursor: pointer;
  overflow: hidden;
  transition: all 0.5s cubic-bezier(0.4, 0, 0.2, 1);
  border-radius: 16px;
  background: #2a2a2a;
  box-shadow: 0 4px 16px rgba(0, 0, 0, 0.15);

  /* Consistent size */
  width: 340px;
  height: 480px;
  flex-shrink: 0;
}

/* Enhanced hover effect - NO ZOOM */
.category-card-new:hover {
  transform: translateY(-8px);
  box-shadow: 0 16px 40px rgba(0, 0, 0, 0.35);
  z-index: 10;
}

.category-img-bg {
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center;
  transition: all 0.6s cubic-bezier(0.4, 0, 0.2, 1);
  filter: grayscale(25%) brightness(0.78) contrast(1.15);
  pointer-events: none;
}

.category-card-new:hover .category-img-bg {
  /* NO ZOOM - just filter change */
  filter: grayscale(0%) brightness(1.05) contrast(1.05);
}

.category-overlay {
  position: absolute;
  inset: 0;
  background: linear-gradient(to bottom,
      rgba(0, 0, 0, 0.15),
      rgba(0, 0, 0, 0.75));
  transition: all 0.6s cubic-bezier(0.4, 0, 0.2, 1);
  pointer-events: none;
}

.category-card-new:hover .category-overlay {
  background: linear-gradient(to bottom,
      transparent 0%,
      rgba(0, 0, 0, 0.2) 100%);
}

.category-label-vertical {
  position: absolute;
  bottom: 45px;
  left: 50%;
  transform: translateX(-50%);

  font-size: 30px;
  font-weight: 900;
  color: #f4c542 !important;
  letter-spacing: 0.06em;
  writing-mode: vertical-rl;
  text-orientation: mixed;
  max-height: 390px;

  text-shadow:
    2px 2px 6px rgba(0, 0, 0, 1),
    0 0 25px rgba(0, 0, 0, 0.7);
  transition: all 0.5s cubic-bezier(0.4, 0, 0.2, 1);
  white-space: nowrap;
  z-index: 10;
  pointer-events: none;
  font-family: "Playfair Display", serif;
}

.category-card-new:hover .category-label-vertical {
  opacity: 0;
  transform: translateX(-50%) translateY(20px);
}

/* Corner decorative elements */
.category-card-new::before {
  content: '';
  position: absolute;
  top: 15px;
  right: 15px;
  width: 40px;
  height: 40px;
  border-top: 3px solid rgba(244, 197, 66, 0.6);
  border-right: 3px solid rgba(244, 197, 66, 0.6);
  z-index: 5;
  opacity: 0;
  transition: all 0.5s cubic-bezier(0.4, 0, 0.2, 1);
}

.category-card-new:hover::before {
  opacity: 1;
  top: 20px;
  right: 20px;
}

.category-card-new::after {
  content: '';
  position: absolute;
  bottom: 15px;
  right: 15px;
  width: 40px;
  height: 40px;
  border-bottom: 3px solid rgba(244, 197, 66, 0.6);
  border-right: 3px solid rgba(244, 197, 66, 0.6);
  z-index: 5;
  opacity: 0;
  transition: all 0.5s cubic-bezier(0.4, 0, 0.2, 1);
}

.category-card-new:hover::after {
  opacity: 1;
  bottom: 20px;
  right: 20px;
}

/* Navigation Buttons */
.carousel-nav-button {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  width: 60px;
  height: 60px;
  border-radius: 50%;
  background: #3b1f23;
  border: 3px solid #f4c542;
  color: #f4c542;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 20;
  transition: all 0.3s ease;
  box-shadow: 0 4px 16px rgba(0, 0, 0, 0.2);
}

.carousel-nav-button:hover {
  background: #f4c542;
  color: #3b1f23;
  transform: translateY(-50%) scale(1.1);
  box-shadow: 0 6px 24px rgba(244, 197, 66, 0.4);
}

.carousel-nav-button:active {
  transform: translateY(-50%) scale(0.95);
}

.carousel-nav-button.disabled {
  opacity: 0.3;
  cursor: not-allowed;
  pointer-events: none;
}

.carousel-nav-button.prev {
  left: -30px;
}

.carousel-nav-button.next {
  right: -30px;
}

.carousel-nav-button svg {
  width: 28px;
  height: 28px;
}

/* Fade animation */
@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(20px);
  }

  to {
    opacity: 1;
    transform: translateY(0);
  }
}

/* Responsive adjustments */
@media (max-width: 1600px) {
  .categories-carousel-container {
    padding: 0 40px;
  }

  .category-card-new {
    width: 320px;
    height: 450px;
  }
}

@media (max-width: 1400px) {
  .categories-title {
    font-size: 56px;
  }

  .categories-carousel-container {
    max-width: 1400px;
  }

  .category-card-new {
    width: 300px;
    height: 420px;
  }

  .carousel-nav-button {
    width: 50px;
    height: 50px;
  }

  .carousel-nav-button svg {
    width: 24px;
    height: 24px;
  }
}

@media (max-width: 968px) {
  .categories-section {
    padding: 60px 0;
  }

  .categories-title {
    font-size: 42px;
    margin-bottom: 40px;
  }

  .categories-carousel-container {
    padding: 0 20px;
  }

  .categories-carousel-box {
    padding: 30px 20px;
  }

  .category-card-new {
    width: 280px;
    height: 400px;
  }

  .category-label-vertical {
    font-size: 26px;
    bottom: 35px;
    max-height: 320px;
  }

  .carousel-nav-button.prev {
    left: -15px;
  }

  .carousel-nav-button.next {
    right: -15px;
  }
}

@media (max-width: 640px) {
  .categories-carousel-box {
    border-width: 3px;
    padding: 20px 15px;
  }

  .category-card-new {
    width: 260px;
    height: 380px;
  }

  .category-card-new:hover .category-img-bg {
    /* No zoom on mobile */
    filter: grayscale(0%) brightness(1.05);
  }

  .category-label-vertical {
    font-size: 24px;
    max-height: 300px;
  }

  .carousel-nav-button {
    width: 45px;
    height: 45px;
  }

  .carousel-nav-button svg {
    width: 20px;
    height: 20px;
  }

  .carousel-nav-button.prev {
    left: -10px;
  }

  .carousel-nav-button.next {
    right: -10px;
  }
}

/* ================= ENSURE ALL SECTIONS HAVE PROPER Z-INDEX ================= */

section {
  position: relative;
  z-index: 10;
  background: #FFF7EC;
}

/* ================= PRODUCTS ================= */

.products-page {
  padding: 120px 6vw;
  background: #FFF7EC;
  min-height: 100vh;
}

.products-title {
  font-family: "Playfair Display", serif;
  font-size: 42px;
  margin-bottom: 60px;
  color: #3b1f23;
}

.products-grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 36px;
}

.product-card {
  background: #ffffff;
  border-radius: 12px;
  overflow: hidden;
}

.product-img {
  position: relative;
  height: 420px;
}

.product-info {
  padding: 20px;
  text-align: center;
}

.product-info h3 {
  font-size: 15px;
  margin-bottom: 8px;
  color: #2c1a1a;
}

.rating {
  font-size: 13px;
  color: #111;
  margin-bottom: 10px;
}

.rating span {
  color: #555;
  margin-left: 6px;
}

.price {
  margin-bottom: 14px;
}

.price .old {
  text-decoration: line-through;
  color: #777;
  margin-right: 6px;
}

.price .new {
  font-weight: 600;
}

.size-select {
  width: 100%;
  padding: 10px;
  margin-bottom: 12px;
  border: 1px solid #ccc;
}

.add-cart {
  width: 100%;
  padding: 12px;
  background: #8b5d5d;
  color: white;
  border-radius: 999px;
  font-size: 14px;
}

/* responsive */
@media (max-width: 1200px) {
  .products-grid {
    grid-template-columns: repeat(2, 1fr);
  }
}

@media (max-width: 600px) {
  .products-grid {
    grid-template-columns: 1fr;
  }
}


/* ================= CART ================= */
.cart-page {
  padding: 120px 8vw;
  min-height: 100vh;
  background: #f9f3e7;
}

.cart-title {
  font-size: 42px;
  margin-bottom: 40px;
}

.empty-cart-text {
  font-size: 20px;
  opacity: 0.7;
}

.cart-items {
  display: flex;
  flex-direction: column;
  gap: 28px;
}

.cart-row {
  display: flex;
  gap: 24px;
  background: #fff;
  padding: 20px;
  border-radius: 16px;
}

.cart-img {
  position: relative;
  width: 120px;
  height: 160px;
}

.cart-info h3 {
  font-size: 18px;
  margin-bottom: 6px;
}

.remove-btn {
  margin-top: 10px;
  background: none;
  color: #8b0000;
  border: none;
  cursor: pointer;
}

.cart-summary {
  margin-top: 50px;
  padding: 30px;
  background: #fff;
  border-radius: 16px;
  max-width: 420px;
}

.clear-btn {
  margin-top: 14px;
  width: 100%;
  padding: 12px;
  border-radius: 999px;
  background: #ccc;
}

.checkout-btn {
  margin-top: 14px;
  width: 100%;
  padding: 14px;
  border-radius: 999px;
  background: #8b5d5d;
  color: white;
}


/* ================= CART DRAWER ================= */

.cart-overlay {
  position: fixed;
  inset: 0;
  background: rgba(0, 0, 0, 0.45);
  z-index: 90;
}

.cart-drawer {
  position: fixed;
  top: 0;
  right: -420px;
  width: 420px;
  height: 100vh;
  background: #fff;
  z-index: 9999;
  padding: 24px;
  transition: right 0.4s ease;
  box-shadow: -20px 0 40px rgba(0, 0, 0, 0.2);
  display: flex;
  flex-direction: column;
}

.cart-drawer.open {
  right: 0;
}

.cart-header {
  padding: 20px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eee;
}

.cart-header h2 {
  font-size: 20px;
  font-weight: 500;
}

.cart-items {
  flex: 1;
  overflow-y: auto;
  padding: 20px;
  display: flex;
  flex-direction: column;
  gap: 20px;
}

.cart-item {
  display: flex;
  gap: 16px;
  margin-bottom: 20px;
}

.cart-img {
  position: relative;
  width: 90px;
  height: 120px;
  border-radius: 6px;
  overflow: hidden;
  background: #f4f4f4;
}

.cart-details {
  flex: 1;
}

.cart-details h4 {
  font-size: 15px;
  font-weight: 500;
  margin-bottom: 4px;
}

.cart-size {
  font-size: 13px;
  color: #777;
  margin-bottom: 6px;
}

.cart-qty-row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 6px 0;
  font-size: 14px;
}

.remove-btn {
  background: none;
  border: none;
  color: #b00020;
  cursor: pointer;
  font-size: 13px;
  padding: 0;
  margin-top: 6px;
}

.empty-cart {
  margin-top: 60px;
  text-align: center;
  opacity: 0.6;
}

.cart-summary {
  padding: 16px 20px;
  border-top: 1px solid #eee;
}

.cart-summary-row {
  display: flex;
  justify-content: space-between;
  font-size: 14px;
  margin-bottom: 6px;
}

.cart-summary-total {
  display: flex;
  justify-content: space-between;
  font-size: 16px;
  font-weight: 600;
  margin-top: 10px;
}

.cart-footer {
  padding: 16px 20px;
  display: flex;
  flex-direction: column;
  gap: 10px;
}

.clear-btn,
.checkout-btn {
  width: 100%;
  padding: 14px;
  border-radius: 30px;
  font-size: 15px;
  cursor: pointer;
  border: none;
}

.clear-btn {
  background: #eee;
}

.checkout-btn {
  background: #000;
  color: #fff;
}

.coupon-box {
  display: flex;
  gap: 10px;
  margin-top: 20px;
}

.coupon-box input {
  flex: 1;
  padding: 10px;
  border: 1px solid #ccc;
}

.coupon-box button {
  padding: 10px 16px;
}


/* ================= REMOVE SCROLLBAR EVERYWHERE ================= */

/* Chrome, Safari, Edge */
*::-webkit-scrollbar {
  display: none;
}

/* Firefox */
* {
  scrollbar-width: none;
}

/* IE / old Edge */
* {
  -ms-overflow-style: none;
}

