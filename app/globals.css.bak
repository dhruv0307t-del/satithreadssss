}

.product-image {
  position: relative;
  width: 100%;
  max-width: 520px;
  height: 620px;
  background: #fff;
  border-radius: 28px;
  overflow: hidden;
}

/* HARD FIX for Next Image */
.product-image>span,
.product-image img {
